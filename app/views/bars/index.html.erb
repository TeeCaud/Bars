<%= link_to "Bars", "/bars" %>
<%= link_to "Drinks", "/drinks" %>
<%= link_to "New Bar", "/bars/new" %>


<h1>Bars</h1>

<% @bars.each_with_index do |bar, index| %>
<div id="bar-<%= index %>">
<h3><%= bar.name %>   <%= link_to "Edit #{bar.name}", "/bars/#{bar.id}/edit" =%></h3>
<p>Created at: <%= bar.created_at %></p>
</div>
<% end %>
